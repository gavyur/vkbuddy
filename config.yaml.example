# VK.com account login:
login: 79123456789
# VK.com account password:
password: supersecret

# Use https? Note that force https checkbox should be disabled in
# VK.com page settings. By default VKBuddy will use HTTPS
#https: False

# VK API application settings. By default using VK Android App settings
# If you want to use your application, specify client_id and client_secret
# below:

#client_id: YOUR_APP_ID_HERE
#client_secret: YOUR_APP_SECRET_HERE

# If you don't want to keep your login and password in config file,
# you can specify access_token and (for non-https auth) secret
# parameters:

#access_token: YOUR_ACCESS_TOKEN_HERE
#secret: YOUR_SECRET_HERE

# VK.com account number for validation:
number: 79123456789

# Keep account online while VKBuddy is running? By default this
# feature is enabled
#online: False

# Log file path. By default use ./vkbuddy.log
#logfile: /var/log/vkbuddy.log

# Debug mode:
#debug: true